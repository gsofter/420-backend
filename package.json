{
  "name": "420-backend",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "UNLICENSED",
  "author": "",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "setup": "psql -f ./prisma/config.sql",
    "migrate:dev": "NODE_ENV=development prisma migrate deploy && prisma generate",
    "seed": "NODE_ENV=development prisma db seed",
    "init:dev": "yarn setup && yarn migrate:dev && yarn seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "hash-table": "ts-node ./data/thcHashTable.ts",
    "postinstall": "yarn hash-table"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@fast-csv/parse": "^4.3.6",
    "@nestjs/common": "8.4.3",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "8.4.3",
    "@nestjs/platform-express": "8.4.3",
    "@prisma/client": "3.12.0",
    "@types/jsonwebtoken": "^8.5.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv-flow": "^3.2.0",
    "ethers": "^5.6.4",
    "fast-csv": "^4.3.6",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5"
  },
  "devDependencies": {
    "@nestjs/cli": "8.2.4",
    "@nestjs/schematics": "8.0.9",
    "@nestjs/testing": "8.4.3",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.26",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.5.1",
    "prettier": "2.6.2",
    "prisma": "3.12.0",
    "run-script-webpack-plugin": "^0.0.11",
    "supertest": "6.2.2",
    "ts-jest": "27.1.4",
    "ts-loader": "9.2.8",
    "ts-node": "10.7.0",
    "tsconfig-paths": "3.14.1",
    "typescript": "4.6.3",
    "webpack": "^5.72.0",
    "webpack-node-externals": "^3.0.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
